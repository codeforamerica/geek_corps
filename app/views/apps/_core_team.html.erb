<div class="side-bar-item">
    <div class="side-bar-box-title">Core Team</div>
    <div class="side-bar-box-content">
      <% @app.core_team_members.each do |organizer|%>
      <div class="supporter-image">
        <%= image_tag organizer.user.person.photo.url :width => "200" %>
        <%=link_to organizer.user.person.name, organizer.user.person %>
      </div>
      <%end%>
      <div class="clear"></div>
      <div class="side-bar-box-more-info supporters-fix"><%= link_to 'Full List', apps_people_path(@app), :class => 'arrow-icon' %></div>
        <div class="clear"></div>
    </div>
</div>
