<div id="feed-wrapper">
  <div id="activity-feed">
  <% comment.comments.order("created_at DESC").each do |feed| %>
    <div class="feed-item">
      <%= activity_image(feed)%>
      <span class="comment-body"><%=feed.text%></span>
      <span class="comment-stamp"><%= activity_type(feed)%>, <%= time_ago_in_words(feed.created_at)%><span>
    </div>
  <% end %>
  </div>
</div>
