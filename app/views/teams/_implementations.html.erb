<div class="sidebar_box sb_container">
    <div class="sidebar_box_title">Regional Teams</div>
    <div class="sidebar_box_content ">
      <% @team.app.regional_teams.each do |team|%>
      <div class="supporter_image">
          <%= link_to image_tag(team.region.photo.url, :alt => team.name, :class => "medium-avatar"), team.to_url %>
      </div>
      <%end%>
      <div class="clear"></div>    
      <div class="sidebar_box_more_info"><%= link_to 'Full List', apps_people_path(@team.app), :class => 'arrow-icon' %></div>
      <div class="clear"></div>
    </div>
</div>
