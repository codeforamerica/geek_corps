<div class="sidebar_box sb_container">
    <div class="sidebar_box_title"><%=@team.region ? @team.region.city : 'Core'%> Team</div>
    <div class="sidebar_box_content ">
      <% @team.team_members.where(:team_role => 'supporter')[0..10].each do |supporter|%>
      <div class="supporter_image">
        <%=link_to (image_tag supporter.user.person.photo.url(:medium), :class=>"medium-avatar", :alt => supporter.user.person.name), supporter.user.person%>
      </div>
      <%end%>
      <div class="clear"></div>    
      <div class="sidebar_box_more_info"><%= link_to 'Full List', team_people_path(@team.name), :class => 'arrow-icon' %></div>
      <div class="clear"></div>
    </div>
</div>

