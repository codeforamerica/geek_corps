<h1>Listing teams</h1>

<table>
  <tr>
    <th>App</th>
    <th>Region</th>
    <th>Team type</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.app_id %></td>
    <td><%= team.region_id %></td>
    <td><%= team.team_type %></td>
    <td><%= team.name %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
